{
    "git": {
        "handle_working_tree": true
    },
    "github": {
        "enableReleaseNotes": true
    },
    "npm": {
        "publish": true
    },
    "baseBranch": "main",
    "releaseBranches": [
        {
            "name": "dev",
            "prerelease": false,
            "enableReleaseNotes": true
        },
        {
            "name": "alpha",
            "prerelease": true,
            "enableReleaseNotes": true
        },
        {
            "name": "beta",
            "prerelease": true,
            "enableReleaseNotes": true
        },
        {
            "name": "rc",
            "prerelease": true,
            "enableReleaseNotes": false
        }
    ]
}